<configuration>
    <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>%d{HH:mm} [%level]: %msg%n</pattern>
        </encoder>
    </appender>

    <logger name="com.goodasssub.gasevents" level="INFO"/>

    <root level="ERROR">
        <appender-ref ref="STDOUT"/>
    </root>
</configuration>
